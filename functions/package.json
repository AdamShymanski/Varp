    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.22.0",
    "firebase-functions-test": "^0.2.0",
    "typescript": "^3.8.0",
    "firebase-admin": "^9.7.0",
    "firebase-functions": "^3.13.2"
  },
  "private": true
